        {
          "component": "ui/TweetButton",
          "text": "buildremote: buildremote.com",
          "url": null
        },
        {
          "component": "ui/GithubButton",
          "user": "BuildRemote",
          "repo": "buildremote"
        }
      ]
    },
    {
      "component": "footer",
      "links": [
        {
          "href": "https://github.com/BuildRemote/buildremote",
          "text": "GitHub"
